#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "\"%1$s\" tartományhoz és \"%2$s\" kódhoz a felsorolás fajtája hiányzik"

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "A választott jellemzőkkel és \"%1$s\" azonosítóval nem található cikk"

#, php-format
msgid "No customer lists domain"
msgstr "Vásárlólisták tartománya hiányzik"

#, php-format
msgid "No customer lists type code"
msgstr "Vásárlólisták típuskódja hiányzik"

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "Nem található tétel ehhez: \"%1$s\" (%2$s)"

#, php-format
msgid "No order item for ID \"%1$s\" found"
msgstr "Nem található rendelési tétel a következő azonosítóval: \"%1$s\""

#, php-format
msgid "No stock for product ID \"%1$s\" and type \"%2$s\" available"
msgstr "\"%1$s\" azonosítószámú \"%2$s\" fajta termék nincs készleten"

#, php-format
msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "A választott jellemzőkkel és \"%1$s\" azonosítóval nem található egyedi cikk"

#, php-format
msgid "Not allowed to access customer data for ID \"%1$s\""
msgstr "Nem engedélyezett a \"%1$s\" azonosítójú vásárló adatainak elérése"

#, php-format
msgid "Number of coupon codes exceeds the limit"
msgstr "A kuponkódok száma meghaladja a megengedett számot"

#, php-format
msgid "Product item with ID \"%1$s\" not found"
msgstr "\"%1$s\" azonosítóval nem található termék"

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "\"%1$s\" kuponkód feltételei nem teljesülnek"

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "\"%1$s\" fajtához és \"%2$s\" azonosítóhoz nem található szolgáltatási tétel"

#, php-format
msgid "Site is not available"
msgstr ""

#, php-format
msgid "Temporary limit reached"
msgstr "Elérte az átmeneti korlátot"

#, php-format
msgid "Temporary order limit reached"
msgstr "Elérte az átmeneti rendelési korlátot"

#, php-format
msgid "The order has already been created"
msgstr "A rendelés már elkészült"

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "\"%1$s\" termékből nincs elég készleten."

#, php-format
msgid "Unable to call method \"%1$s\""
msgstr "Nem lehet, meghívni a \"%1$s\" eljárást"

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Nem lehet meghívni \"%1$s::%2$s\" eljárást"

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "Nem lehet létrehozni \"%1$s\" szolgátatót \"%2$s\" szolgáltatásazonosítóhoz"

#, php-format
msgid "Unable to find product \"%1$s\""
msgstr "\"%1$s\" termék nem található"

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Ismeretlen jellemzők: \"%1$s\""

#, php-format
msgid "You are not allowed to manage groups"
msgstr "Nincs engedélye a csoportok kezelésére"

#, php-format
msgid "You can only add a review if you have ordered a product"
msgstr "Csak akkor adhat hozzá véleményt, ha megrendelt egy terméket"
