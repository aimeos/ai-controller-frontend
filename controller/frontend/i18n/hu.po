msgid "%013.2F"
msgstr "%013.2F"

#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "\"%1$s\" azonosítóval nem adhat hozzá terméket"

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "\"%1$s\" elérhető jellemző nem egyezik a/az \"%2$s\" kapott jellemzővel"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "\"%1$d\" pozícióban a kosárba tett termék nem módosítható."

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "\"%1$d\" pozícióban a kosárba tett termék nem törölhető kézzel"

#, php-format
msgid "Category is not available"
msgstr ""

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "A(z) \"%1$s\" osztály nem valósítja meg a(z) \"%2$s\" felület osztályt"

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "A/Az \"%1$s\" osztály nem elérhető."

#, php-format
msgid "Controller path is empty"
msgstr "A kontroller útvonal üres"

#, php-format
msgid "Coupon code \"%1$s\" is invalid"
msgstr "Érvénytelen kupon kód: \"%1$s\""

#, php-format
msgid "Coupon code \"%1$s\" is invalid or not available any more"
msgstr "Érvénytelen vagy nem elérhető kupon kód: \"%1$s\""

#, php-format
msgid "Coupon for code \"%1$s\" is not available any more"
msgstr "Nem elérhető kupon kód: \"%1$s\""

#, php-format
msgid "Domain \"%1$s\" is not supported"
msgstr "\"%1$s\" tartomány nem támogatott"

#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "Érvénytelen \"%1$s\" hivatkozások a \"%2$s\" azonosítójú termékre"

#, php-format
msgid "Invalid \"%1$s\" references for product with ID %2$s"
msgstr "Érvénytelen \"%1$s\" hivatkozások a %2$s azonosítójú termékre"

#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Érvénytelen címtulajdonságok! Kérjük, ellenőrizze a bevitelt!"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Érvénytelen karakter a/az \"%1$s\" osztály nevében"

#, php-format
msgid "Invalid characters in controller name \"%1$s\""
msgstr "Érvénytelen karakterek a/az \"%1$s\" vezérlő nevében"

