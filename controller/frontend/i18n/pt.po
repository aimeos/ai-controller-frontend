#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "Não é permitido adicionar o produto com ID \"%1$s\""

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "IDs do atributo disponível \"%1$s\" são diferentes do IDs de atributo informados \"%2$s\""

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "O item do carrinho na posição \"%1$d\" não pode ser alterado"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "O item do carrinho na posição \"%1$d\" não pode ser removido manualmente"

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "A classe \"%1$s\" não implementa a interface \"%2$s\""

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "Classe \"%1$s\" não disponível"

#, php-format
msgid "Controller path is empty"
msgstr "Caminho do controlador está vazio"

#, php-format
msgid "Coupon code \"%1$s\" is invalid"
msgstr "Código do cupom \"%1$s\" é inválido"

#, php-format
msgid "Coupon code \"%1$s\" is invalid or not available any more"
msgstr "Código do cupom \"%1$s\" é inválido ou temporariamente indisponível"

#, php-format
msgid "Coupon for code \"%1$s\" is not available any more"
msgstr "Cupom para o código \"%1$s\" não estão mais disponível"

#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "Referência \"%1$s\" inválida para o produto com ID \"%2$s\""

#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Propriedades de endereço inválidas, por favor verifique"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Caracteres inválidos no nome da classe \"%1$s\""

#, php-format
msgid "Invalid characters in controller name \"%1$s\" in \"%2$s\""
msgstr "Caracteres inválidos no nome do controlador \"%1$s\" em \"%2$s\""

#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "Domínio \"%1$s\" inválido"

#, php-format
msgid "Invalid factory \"%1$s\""
msgstr "Fábrica inválida \"%1$s\""

#, php-format
msgid "Invalid value for address type \"%1$s\""
msgstr "Tipo de endereço \"%1$s\" inválido"

#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "Tipo de lista para o domínio \"%1$s\" and code \"%2$s\" não encontrado"

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "Nenhum item encontrado para os atributos selecionados e ID do produto \"%1$s\""

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "Nenhum item encontrado para \"%1$s\" (%2$s)"

#, php-format
msgid "No stock for product ID \"%1$s\" and type \"%2$s\" available"
msgstr ""

#, php-format
msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "Nenhum item único encontrado para os atributos selecionados e ID do produto \"%1$s\""

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "Os requisitos para o código do cupom \"%1$s\" não foram cumpridos"

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "Item de serviço do tipo \"%1$s\" e ID \"%2$s\" não encontrado"

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "Não há produtos \"%1$s\" em estoque"

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Incapaz de chamar o método \"%1$s::%2$s\""

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "Incapaz de criar provedor \"%1$s\" para o serviço com ID \"%2$s\""

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Atributo desconhecido \"%1$s\""
