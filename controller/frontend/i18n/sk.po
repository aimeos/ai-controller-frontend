#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "Pridanie produktu s ID  \"%1$s\" nie je povolené"

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "Dostupné atribút ID  \"%1$s\" sa nezhodujú s danými parametrami ID  \"%2$s\""

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "Položka košíku na pozícií  \"%1$d\" nemôže byť zmenená"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "Položka v košíku na pozícií  \"%1$d\" nemôŽe byť ručne zmazaná"

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "Trieda  \"%1$s\" neimplementuje rozhranie  \"%2$s\""

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "Trieda  \"%1$s\" nie je dostupná"

#, php-format
msgid "Controller path is empty"
