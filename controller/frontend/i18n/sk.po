#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Neplatné adresné vlastnosti, prosím skontrolujte to"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Neplatné znaky v triede  \"%1$s\""

#, php-format
msgid "Invalid characters in controller name \"%1$s\" in \"%2$s\""
msgstr "Neplatné znaky v ovládači  \"%1$s\" v \"%2$s\""

#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "Neplatná doména  \"%1$s\""

#, php-format
msgid "Invalid factory \"%1$s\""
