msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "Žiaden unikátny artikel sa nenašiel pre zvolené kritéria a ID produktu \"%1$s\""

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "Požiadavky pre kód kupónu \"%1$s\" ste nesplnili"

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "Položka služby pre typ \"%1$s\" a ID \"%2$s\" nebol nájdený"

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "Nie je dostatok tovaru \"%1$s\" na sklade"

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Nepodailo sa zavolať metódu \"%1$s::%2$s\""

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "Nepodarilo sa vytvoriť poskytovateľa \"%1$s\" pre službu s ID \"%2$s\""

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Neznáme atribúty \"%1$s\""
