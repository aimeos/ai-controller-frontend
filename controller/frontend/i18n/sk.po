#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "Typ záznamu nenájdený pre doménu \"%1$s\" a kód \"%2$s\""

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "Nebol nájdený žiaden artikel pre zvolené kritéria a ID produktu \"%1$s\""

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "Žiadná položka nebola nájdená pre \"%1$s\" (%2$s)"

#, php-format
msgid "No stock for product ID \"%1$s\" and type \"%2$s\" available"
msgstr ""

