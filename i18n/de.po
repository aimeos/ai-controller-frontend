msgstr "Die Voraussetzungen für den Gutscheincode \"%1$s\" sind nicht erfüllt"

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "Es wurde kein Eintrag für den Service mit der ID \"%2$s\" und dem Typ \"%1$s\" gefunden"

#, php-format
msgid "Site is not available"
msgstr "Mandant ist nicht verfügbar"

#, php-format
msgid "Temporary limit reached"
msgstr "Zeitweiliges Limit erreicht"

#, php-format
msgid "Temporary order limit reached"
msgstr "Zeitweiliges Limit für Bestellungen erreicht"

#, php-format
msgid "The order has already been created"
msgstr "Die Bestellung ist schon gespeichert worden"

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "Es sind leider nicht genügend Produkte \"%1$s\" vorhanden"

#, php-format
msgid "Unable to call method \"%1$s\""
msgstr "Methode \"%1$s\" kann nicht aufgerufen werden"

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Die Methode \"%1$s::%2$s\" konnte nicht aufgerufen werden"

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "Es konnte kein Service \"%1$s\" mit der ID \"%2$s\" erstellt werden"

#, php-format
msgid "Unable to find product \"%1$s\""
msgstr "Produkt \"%1$s\" konnte nicht gefunden werden"

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Die übergebenen Attribute \"%1$s\" konnten nicht zugeordnet werden"

#, php-format
msgid "You are not allowed to manage groups"
msgstr "Sie dürfen keine Gruppen verwalten"

#, php-format
msgid "You can only add a review if you have ordered a product"
msgstr "Sie können nur eine Bewertung für gekaufte Produkte abgeben"
