msgid "%013.2F"
msgstr "%013.2F"

#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "ID \"%1$s\"로 제품을 추가할 수 없습니다."

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "사용 가능한 특성 ID \"%1$s\"가 지정된 특성 ID \"%2$s\"와 일치하지 않습니다."

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "\"%1$d\" 위치의 바구니 항목을 변경할 수 없습니다."

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "\"%1$d\" 위치에 있는 바구니 항목을 수동으로 삭제할 수 없습니다."

#, php-format
msgid "Category is not available"
msgstr "카테고리를 사용할 수 없습니다."

#, php-format
msgid "Class \"%1$s\" does not implement \"%2$s\""
msgstr "\"%1$s\" 클래스가 \"%2$s\"를 구현하지 않습니다."

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "\"%1$s\" 클래스가 \"%2$s\" 인터페이스를 구현하지 않습니다."

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "\"%1$s\" 클래스를 사용할 수 없습니다."

#, php-format
msgid "Class \"%1$s\" not found"
msgstr "\"%1$s\" 클래스를 찾을 수 없습니다."

#, php-format
msgid "Controller path is empty"
msgstr "컨트롤러 경로가 비어 있습니다."

#, php-format
msgid "Coupon code \"%1$s\" is invalid"
msgstr "쿠폰 코드 \"%1$s\"가 잘못되었습니다."

#, php-format
msgid "Coupon code \"%1$s\" is invalid or not available any more"
msgstr "쿠폰 코드 \"%1$s\"가 유효하지 않거나 더 이상 사용할 수 없습니다."

#, php-format
msgid "Coupon for code \"%1$s\" is not available any more"
msgstr "\"%1$s\" 코드에 대한 쿠폰을 더 이상 사용할 수 없습니다."

#, php-format
msgid "Domain \"%1$s\" is not supported"
msgstr "도메인 \"%1$s\"는 지원되지 않습니다."

#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "ID가 \"%2$s\"인 제품에 대한 잘못된 \"%1$s\" 참조입니다."

#, php-format
msgid "Invalid \"%1$s\" references for product with ID %2$s"
msgstr "ID가 %2$s인 제품에 대한 잘못된 \"%1$s\" 참조입니다."

#, php-format
msgid "Invalid address properties, please check your input"
msgstr "잘못된 주소 속성, 입력 내용을 확인해 주세요."

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "클래스 이름 \"%1$s\"에 잘못된 문자가 있습니다."

#, php-format
msgid "Invalid characters in controller name \"%1$s\""
msgstr "컨트롤러 이름 \"%1$s\"에 잘못된 문자가 있습니다."

#, php-format
msgid "Invalid characters in controller name \"%1$s\" in \"%2$s\""
msgstr "\"%2$s\"의 컨트롤러 이름 \"%1$s\"에 잘못된 문자가 있습니다."

#, php-format
msgid "Invalid class name \"%1$s\""
msgstr "잘못된 클래스 이름 \"%1$s\""

#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "잘못된 도메인 \"%1$s\""

#, php-format
msgid "Invalid factory \"%1$s\""
msgstr "잘못된 공장 \"%1$s\""

#, php-format
msgid "Invalid price value \"%1$s\""
msgstr "잘못된 가격 값 \"%1$s\""

msgid "Invalid service attributes"
msgstr "잘못된 서비스 속성"

#, php-format
msgid "Invalid value for address type \"%1$s\""
msgstr "주소 유형 \"%1$s\"에 대한 잘못된 값입니다."

#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "도메인 \"%1$s\" 및 코드 \"%2$s\"의 목록 유형을 찾을 수 없습니다."

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "선택한 속성 및 제품 ID \"%1$s\"에 대한 문서를 찾을 수 없습니다."

#, php-format
msgid "No customer lists domain"
msgstr "고객이 도메인을 나열하지 않음"

#, php-format
msgid "No customer lists type code"
msgstr "고객이 유형 코드를 나열하지 않음"

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "\"%1$s\"(%2$s)에 대한 항목을 찾을 수 없습니다."

#, php-format
msgid "No order item for ID \"%1$s\" found"
msgstr "ID \"%1$s\"에 대한 주문 항목을 찾을 수 없습니다."

#, php-format
msgid "No stock for product ID \"%1$s\" and type \"%2$s\" available"
msgstr "제품 ID \"%1$s\" 및 유형 \"%2$s\"에 대한 재고가 없습니다."

#, php-format
msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "선택한 속성 및 제품 ID \"%1$s\"에 대한 고유 문서를 찾을 수 없습니다."

#, php-format
msgid "Not allowed to access customer data for ID \"%1$s\""
msgstr "ID \"%1$s\"에 대한 고객 데이터에 액세스할 수 없습니다."

#, php-format
msgid "Number of coupon codes exceeds the limit"
msgstr "쿠폰 코드 개수가 한도를 초과했습니다."

#, php-format
msgid "Product item with ID \"%1$s\" not found"
msgstr "ID가 \"%1$s\"인 제품 항목을 찾을 수 없습니다."

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "쿠폰 코드 \"%1$s\"에 대한 요구 사항이 충족되지 않습니다."

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "\"%1$s\" 유형 및 ID \"%2$s\"에 대한 서비스 항목을 찾을 수 없습니다."

#, php-format
msgid "Site is not available"
msgstr "사이트를 사용할 수 없습니다."

#, php-format
msgid "Temporary limit reached"
msgstr "임시 한도에 도달했습니다."

#, php-format
msgid "Temporary order limit reached"
msgstr "임시 주문 한도에 도달했습니다."

#, php-format
msgid "The order has already been created"
msgstr "주문이 이미 생성되었습니다."

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "재고가 \"%1$s\" 제품이 부족합니다."

#, php-format
msgid "Unable to call method \"%1$s\""
msgstr "\"%1$s\" 메서드를 호출할 수 없습니다."

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "\"%1$s::%2$s\" 메서드를 호출할 수 없습니다."

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "ID가 \"%2$s\"인 서비스에 대해 공급자 \"%1$s\"를 만들 수 없습니다."

#, php-format
msgid "Unable to find category \"%1$s\""
msgstr "\"%1$s\" 범주를 찾을 수 없습니다."

#, php-format
msgid "Unable to find product \"%1$s\""
msgstr "\"%1$s\" 제품을 찾을 수 없습니다."

#, php-format
msgid "Unable to find supplier \"%1$s\""
msgstr "공급업체 \"%1$s\"를 찾을 수 없습니다."

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "알 수 없는 속성 \"%1$s\""

msgid "Unknown service attributes"
msgstr "알 수 없는 서비스 속성"

#, php-format
msgid "You are not allowed to manage groups"
msgstr "그룹을 관리할 수 없습니다."

#, php-format
msgid "You can only add a review if you have ordered a product"
msgstr "제품을 주문한 경우에만 리뷰를 추가할 수 있습니다."
